{
  "name": "windy-plugin-heat-units",
  "version": "1.0.21",
  "description": "Agricultural Heat Unit (Growing Degree Days) calculator and visualization plugin for Windy.com",
  "main": "dist/plugin.js",
  "homepage": "https://github.com/crop-crusaders/windy-plugin-heat-units#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/crop-crusaders/windy-plugin-heat-units.git"
  },
  "bugs": {
    "url": "https://github.com/crop-crusaders/windy-plugin-heat-units/issues"
  },
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "test": "echo \"No tests specified yet\" && exit 0",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "lint:fix": "eslint src --ext .ts,.js,.svelte --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/ windy-plugin-heat-units.tar",
    "update:metadata": "node scripts/update-plugin-metadata.mjs",
    "package": "node scripts/package-plugin.mjs",
    "prerelease": "npm run lint && npm run type-check",
    "release": "scripts/release.sh",
    "dev": "npm run start",
    "open:dev": "node scripts/open-windy-dev.mjs",
    "check:api-key": "node scripts/check-api-key.mjs",
    "check:plugin-url": "node scripts/check-plugin-url.mjs",
    "check:ssl": "node scripts/check-ssl.mjs",
    "ensure:live": "node scripts/ensure-plugin-live.mjs"
  },
  "keywords": [
    "windy",
    "plugin",
    "agriculture",
    "heat-units",
    "growing-degree-days",
    "farming",
    "weather",
    "crop-monitoring",
    "agricultural-tools",
    "gdd",
    "temperature",
    "climate",
    "precision-agriculture"
  ],
  "author": {
    "name": "crop-crusaders",
    "url": "https://github.com/crop-crusaders"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/leaflet": "^1.9.20",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-svelte": "^2.46.0",
    "leaflet": "^1.9.4",
    "rollup": "^4.28.1",
    "rollup-plugin-svelte": "^7.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "svelte": "^4.2.19",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "chrome-launcher": "^1.2.1"
  },
  "type": "module"
}
